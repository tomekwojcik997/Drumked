{
  "rules": {
    "no-unnecessary-class": [
      true,
      "allow-constructor-only",
      "allow-static-only",
      "allow-empty-class"
    ],
    "member-access": [true, "no-public"],
    "member-ordering": [true, "variables-before-functions"],
    "prefer-readonly": true,
    "adjacent-overload-signatures": true,
    "unified-signatures": true,
    "prefer-function-over-method": false,
    "no-unbound-method": false,
    "no-invalid-this": false,
    "no-this-assignment": true,
    "unnecessary-constructor": false,
    "no-duplicate-super": true,
    "new-parens": true,
    "no-misused-new": true,
    "no-construct": true,
    "no-empty-interface": false,
    "prefer-method-signature": true,
    "interface-over-type-literal": true,
    "function-constructor": true,
    "no-parameter-reassignment": true,
    "no-void-expression": false,
    "return-undefined": true,
    "no-arg": true,
    "only-arrow-functions": true,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "arrow-return-shorthand": [true, "multiline"],
    "no-unnecessary-callback-wrapper": false,
    "unnecessary-bind": true,
    "promise-function-async": true,
    "await-promise": true,
    "no-return-await": true,
    "no-floating-promises": false,
    "prefer-const": true,
    "no-var-keyword": true,
    "no-shadowed-variable": [
      true,
      {
        "temporalDeadZone": false
      }
    ],
    "one-variable-per-declaration": false,
    "no-duplicate-variable": [true, "check-parameters"],
    "no-unnecessary-initializer": true,
    "no-implicit-dependencies": false,
    "no-import-side-effect": false,
    "ordered-imports": [
      false,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive",
        "grouped-imports": false
      }
    ],
    "no-duplicate-imports": true,
    "import-blacklist": [true, "rxjs/Rx"],
    "no-require-imports": true,
    "no-default-import": true,
    "no-default-export": true,
    "no-reference": true,
    "typedef": [true, "call-signature", "property-declaration"],
    "no-inferrable-types": true,
    "use-default-type-parameter": true,
    "no-object-literal-type-assertion": false,
    "no-angle-bracket-type-assertion": true,
    "no-unnecessary-type-assertion": true,
    "callable-types": true,
    "no-null-keyword": false,
    "no-non-null-assertion": true,
    "array-type": [true, "array"],
    "prefer-object-spread": true,
    "object-literal-shorthand": true,
    "object-literal-key-quotes": [true, "as-needed"],
    "no-string-literal": false,
    "quotemark": [true, "single", "avoid-template", "avoid-escape"],
    "prefer-template": false,
    "no-invalid-template-strings": true,
    "triple-equals": [true, "allow-null-check"],
    "increment-decrement": [true, "allow-post"],
    "restrict-plus-operands": false,
    "binary-expression-operand-order": true,
    "no-dynamic-delete": false,
    "no-bitwise": true,
    "use-isnan": true,
    "no-conditional-assignment": false,
    "prefer-conditional-expression": [true, "check-else-if"],
    "no-boolean-literal-compare": true,
    "strict-boolean-expressions": [
      false,
      "allow-undefined-union",
      "allow-string",
      "allow-enum",
      "allow-number",
      "allow-mix"
    ],
    "prefer-while": true,
    "prefer-for-of": true,
    "forin": true,
    "no-for-in-array": true,
    "switch-default": false,
    "no-switch-case-fall-through": true,
    "no-duplicate-switch-case": true,
    "no-unsafe-finally": true,
    "encoding": true,
    "cyclomatic-complexity": [true, 20],
    "max-file-line-count": [true, 1000],
    "max-line-length": false,
    "indent": [true, "spaces", 2],
    "eofline": true,
    "curly": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      },
      {
        "call-signature": "onespace",
        "index-signature": "onespace",
        "parameter": "onespace",
        "property-declaration": "onespace",
        "variable-declaration": "onespace"
      }
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always",
        "method": "never",
        "constructor": "never"
      }
    ],
    "import-spacing": true,
    "no-trailing-whitespace": true,
    "newline-before-return": false,
    "newline-per-chained-call": false,
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace",
      "check-else",
      "check-catch",
      "check-finally"
    ],
    "no-consecutive-blank-lines": [true, 1],
    "semicolon": [true, "always", "strict-bound-class-methods"],
    "align": [true, "elements", "members", "parameters", "statements"],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never",
        "esSpecCompliant": true
      }
    ],
    "file-name-casing": [true, "kebab-case"],
    "class-name": true,
    "interface-name": [true, "never-prefix"],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "ban-keywords"
    ],
    "comment-type": [true, "singleline", "doc"],
    "comment-format": [true, "check-space"],
    "jsdoc-format": [true, "check-multiline-start"],
    "no-redundant-jsdoc": true,
    "ban-ts-ignore": true,
    "no-console": [true, "log", "debug", "info", "time", "timeEnd", "trace"],
    "no-debugger": true,
    "no-eval": true,
    "no-string-throw": true,
    "no-namespace": true,
    "no-internal-module": true,
    "number-literal-format": true,
    "radix": true,
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "no-empty": false,
    "no-sparse-arrays": true,
    "ban-comma-operator": true
  }
}
